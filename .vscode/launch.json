{
	"version": "0.2.0",
	"configurations":
	[
		{
			"name": "(lldb) Запустить",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/push_swap",
			"args": 
			[
				"5",
				"1",
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [ { "name": "squid", "value": "clam" } ],
			"externalConsole": false,
			"MIMode": "lldb"
		},
		{
			"name": "(lldb) Подключить",
			"type": "lldb",
			"request": "attach",
			"program": "${workspaceFolder}/push_swap}",
			"processId": "${command:pickProcess}",
			"MIMode": "lldb"
		}
	]
}

